@model List<PortfolioCore.Entities.Skill>

@{
    ViewData["Title"] = "SkillList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>yetenekler</h1>

<table class="table table-bordered">
	<tr>
		<th>#</th>
		<th>yetenek adi</th>
		<th>deger</th>
		<th>sil</th>
		<th>guncelle</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.SkillId</td>
			<td>@item.SkillName</td>
			<td>@item.SkillValue</td>
			<td><a href="/Skill/DeleteSkill/@item.SkillId" class="btn btn-danger">sil</a></td>
			<td><a href="/Skill/UpdateSkill/@item.SkillId" class="btn btn-success">guncelle</a></td>
		</tr>
	}
</table>
<a href="/Skill/CreateSkill/" class="btn btn-primary">yeni yetenek girisi</a>

