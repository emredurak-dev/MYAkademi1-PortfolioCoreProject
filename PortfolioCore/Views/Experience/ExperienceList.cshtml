@model List<PortfolioCore.Entities.Experience>
@{
	ViewData["Title"] = "ExperienceList";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>deneyimler</h1>

<table class="table table-bordered">
	<tr>
		<th>#</th>
		<th>baslik</th>
		<th>alt baslik</th>
		<th>aciklama</th>
		<th>tarih</th>
		<th>sil</th>
		<th>guncelle</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.ExperienceId</td>
			<td>@item.Title</td>
			<td>@item.SubTitle</td>
			<td>@item.Description</td>
			<td>@item.ExperienceDate</td>
			<td><a href="/Experience/DeleteExperience/@item.ExperienceId" class="btn btn-danger">sil</a></td>
			<td><a href="/Experience/UpdateExperience/@item.ExperienceId" class="btn btn-success">guncelle</a></td>
		</tr>
	}
</table>
<a href="/Experience/CreateExperience/" class="btn btn-primary">yeni deneyim girisi</a>
